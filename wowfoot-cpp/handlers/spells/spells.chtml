
<p>List spells:</p>

% if(mPair) {
<table class="tab" summary="spells">
<!-- Title row -->
<tr><td>ID</td><td>Name</td></tr>
%  int count = 0;
%  while(mPair->hasNext() && count < 100) {
%   const Spell& s(mPair->next());
<tr>
<td><%=s.id%></td>
<td><a href="spell=<%=s.id%>"><%=s.name%></a></td>
</tr>
%   count++;
%  }
</table>
<p><%=count%> spells.</p>
% }
