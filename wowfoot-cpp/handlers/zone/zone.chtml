<%
int id = toInt(urlPart);
WorldMapAreas::ptr a = gWorldMapAreas.find(id);
AreaTable::ptr ap = gAreaTable.find(id);
if(a) {
	if(ap) {
		mTitle = ap->name;
	} else {
		mTitle = a->name;
	}
} else {
	mTitle = urlPart;
	returnCode = 404;
}
%>

<p>Zone: <%ESCAPE(mTitle);%></p>

% if(ap) {
<p>
<img id="map" width="40%" src="output/<%= a->name %>.jpeg" alt="<%= a->name %>">
</p>
% } else {
<%= id %> not found.
% }

% returnCode = mapSizeChtml().run(stream);
