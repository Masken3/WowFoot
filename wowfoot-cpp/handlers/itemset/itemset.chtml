<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
	<title><%= mTitle %> - Item Set - WowFoot</title>
	<LINK href="static/style.css" rel="stylesheet" type="text/css">
	<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body>

% if(a) {
<p><span class="itemName">Item Set: <%= mTitle %></span></p>

<p>Items:
% for(size_t i=0; i<NUMOF(a->item); i++) { int id = a->item[i]; if(id != 0) {
<%NAMELINK(gItems, id);%><br>
% } }
</p>

% returnCode = tabsChtml::run(stream);
% } else {
<p><%= mTitle %> not found.</p>
% }

</body>
</html>
